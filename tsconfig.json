{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "outDir": "dist",
    "moduleResolution": "node",
    "target": "es5",
    "downlevelIteration": true,
    "sourceMap": false,
    "declaration": true,
    "strictNullChecks": false,
  },
  "include": ["src/**/*"],
  "exclude": [
    "node_modules",
    "**/*.test.*",
    ".eslintrc.js",
    "examples/**"
  ]
}
